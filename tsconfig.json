{
	"compilerOptions": {
    "target": "ESNext",
		"baseUrl": "./src",
		"rootDir": "./src",
		"noEmit": false,
    "types": ["webpack/module", "node"],
    "paths": {
      "_/*": ["./*"],
      "Types/*": ["../node_modules/bc-deeplib/.types/*"],
      "Static/*": ["../node_modules/bc-deeplib/dist/Static/*"]
    },
    "lib": [
      "ESNext",
      "DOM"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "noEmitOnError": true,
    "sourceMap": true,
    "declaration": false,
    "removeComments": true,
    "strict": true,
    "noImplicitOverride": false,
    "noImplicitReturns": true,
	},
	"include": [
    "node_modules/bc-stubs/bc/**/*.d.ts",
    "node_modules/bc-deeplib/**/*.d.ts",
    "./src/**/*",
    ".types/**/*.d.ts"
	]
}
