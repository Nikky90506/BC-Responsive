{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noEmitOnError": true,
    "sourceMap": true,
    "declaration": false,
    "removeComments": true,
    "strict": true,
    "noImplicitOverride": false,
    "noImplicitReturns": true,
    "paths": {
      "_/*": [
        "./src/*"
      ],
      "@Types/*": [
        "./node_modules/bc-deeplib/.types/*"
      ],
      "@Static/*": [
        "./node_modules/bc-deeplib/dist/Static/*"
      ]
    }
  }
}
